<!-- groups -->

<div class="card">
	<div class="card_corner_cut_out"></div>
	<%= render "token/avatar", token: security_token, position: :token_card_link %>
  <div class="vertical_padding" align="center">
    <%= link_to "Groups with #{security_token}", search_path(query: security_token) %>
  </div>
</div>

<% for group in @groups %>
  <%= render "groups/card", group: group %>
<% end %>


